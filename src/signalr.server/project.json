{
	"version": "1.1.0",
	"description": "signalr.server",
	"authors": [ "Q2 OAO Team" ],
	"publishable": true,
  "dependencies": {
    "Microsoft.AspNetCore.Hosting.WindowsServices": "1.1.0-*",
